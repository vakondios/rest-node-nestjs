{
  "name": "mycv",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=development nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch --maxWorkers=1",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest .e2e-spec.ts$",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "compose:down": "docker-compose down --volumes",
    "compose:up": "docker-compose -f docker-compose.dev.yml up -d",
    "typeorm:drop": " node --require ts-node/register ./node_modules/typeorm/cli.js schema:drop -d ./src/_config/typeorm.migration.config.ts",
    "doc": "npx @compodoc/compodoc -p tsconfig.json --includesName Documentation --includes wiki/docs --theme stripe",
    "document": "npx @compodoc/compodoc -p tsconfig.json --includesName Documentation --includes wiki/docs --theme stripe -s",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/typeOrm.config.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./src/typeOrm.config.ts migration:generate ./migrations/migration/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./migrations/migration/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./src/typeOrm.config.ts migration:revert",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.14",
    "@nestjs/typeorm": "^10.0.0",
    "@types/cookie-session": "^2.0.46",
    "@types/express-session": "^1.17.9",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "joi": "^17.11.0",
    "nest-winston": "^1.9.4",
    "nestjs-cls": "^3.6.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.6",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": [
      ".*\\.spec\\.ts$",
      ".*\\-spec\\.ts$"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter",
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ],
    "moduleNameMapper": {
      "@app/(.*)": "<rootDir>/src/$1",
      "@app": "<rootDir>/src",
      "@test/(.*)$": "<rootDir>/test/$1"
    }
  }
}
